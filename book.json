{
    "title": "Learning Advanced Mathematics with Python",
    "author": "Johannes Liu",
    "description": "A Cookbook for SymPy",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Home Page": "https://github.com/johannesliu"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "github",
        "github-buttons",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon",
        "katex",
        "expandable-chapters",
        "copy-code-button",
        "search-plus",
        "page-copyright"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/JohannesLiu"
        },
        "github-buttons": {
            "buttons": [{
                "user": "JohannesLiu",
                "repo": "Learning-Advanced-Mathematics-with-Python",
                "type": "star",
                "size": "small",
                "count": true
                }
            ]
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                "google", "facebook", "weibo", "twitter",
                "qq", "qzone", "linkedin", "pocket"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon":{
            "shortcut": "./source/images/favicon.jpg",
            "bookmark": "./source/images/favicon.jpg",
            "appleTouch": "./source/images/apple-touch-icon.jpg",
            "appleTouchMore": {
                "120x120": "./source/images/apple-touch-icon.jpg",
                "180x180": "./source/images/apple-touch-icon.jpg"
            }
        },
        "page-copyright": {
            "description": "modified at",
            "signature": "",
            "wisdom": "Designer info",
            "format": "YYYY-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; JohannesLiu",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "style": "normal",
            "noPowered": true
          }
    }
